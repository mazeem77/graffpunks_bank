{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon ./server.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@dfuse/client": "^0.3.21",
