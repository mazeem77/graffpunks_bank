    "body-parser": "^1.18.3",
    "dotenv": "^10.0.0",
    "emoji-regex": "^7.0.0",
    "eosjs": "^22.1.0",
    "express": "^4.16.3",
    "human-interval": "^0.1.6",
    "humanize-duration": "^3.15.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "lz-string": "^1.4.4",
    "moment": "^2.22.2",
    "mongoose": "^5.9.29",
    "node-fetch": "^2.6.6",
    "node-polyglot": "^2.3.0",
    "node-schedule": "^2.1.0",
    "numeral": "^2.0.6",
